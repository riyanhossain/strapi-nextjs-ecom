{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "total_price": {
      "type": "integer",
      "required": true
    },
    "order_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "shipped",
        "delivered",
        "cancelled"
      ],
      "default": "pending",
      "required": true
    },
    "order_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order-item.order-item",
      "mappedBy": "order"
    }
  }
}
